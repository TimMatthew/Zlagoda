        <Label layoutX="128.0" layoutY="31.0" prefHeight="25.0" prefWidth="139.0" text="Select the loyal client ">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Button fx:id="noClientButton" layoutX="140.0" layoutY="206.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="113.0" text="No loyal clients" />
        <Button fx:id="selectedClientButton" layoutX="140.0" layoutY="157.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="113.0" text="Select client" visible="false" />
    </children>
</AnchorPane>
