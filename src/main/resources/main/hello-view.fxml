<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="318.0" prefWidth="318.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>

  <Label fx:id="welcomeText" />
   <TextArea prefHeight="31.0" prefWidth="278.0" />
   <TextArea prefHeight="31.0" prefWidth="278.0" />
   <Button mnemonicParsing="false" onAction="#onHelloButtonClick" prefHeight="34.0" prefWidth="133.0" text="Перемога!">
      <font>
         <Font size="14.0" />
      </font>
   </Button>
   <Button mnemonicParsing="false" onAction="#onZradaButtonClick" prefHeight="34.0" prefWidth="133.0" text="Зрада!">
      <font>
         <Font size="14.0" />
      </font>
   </Button>
</VBox>
